-- Moderator Detection

for _,Player in pairs(game.Players:GetChildren()) do
    print('checking if '.. Player.Name .. ' is a moderator')
    if (Player:GetRoleInGroup(10569041) ~= 'Access' and Player:IsInGroup(10569041)) or Player.Backpack:FindFirstChild('eggyweggyObelisk') ~= nil then
        Notification.new("error", "Possible Moderator Detected", ("@(" .. Player.Name ..")" .. " meets one or more conditions to be a moderator."))
    end
end

game.Players.PlayerAdded:Connect(function(Player)
    if (Player:GetRoleInGroup(10569041) ~= 'Access' and Player:IsInGroup(10569041)) or Player.Backpack:FindFirstChild('eggyweggyObelisk') ~= nil then
        Notification.new("error", "Possible Moderator Detected", ("@(" .. Player.Name ..")" .. " meets one or more conditions to be a moderator."))
    end
end)

game.Players.PlayerRemoving:Connect(function(Player)
    if (Player:GetRoleInGroup(10569041) ~= 'Access' and Player:IsInGroup(10569041)) or Player.Backpack:FindFirstChild('eggyweggyObelisk') ~= nil then
        Notification.new("error", "Moderator Left", ("@(" .. Player.Name ..")" .. " has left the game."))
    end
end)

-- Illusionist Notifier

local ScriptContext = game:GetService("ScriptContext")

for i,v in next, getconnections(ScriptContext.Error) do
    v:Disable()
end

local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local RunService = game:GetService("RunService")

local illusionists = {}
local illusionistCounter = 0

Check = function(plr)
    if plr == Players.LocalPlayer then return false end
    if not plr.Character then return false end

    if plr.Character:FindFirstChild("Observe") then
        return true
    else
        if plr.Backpack:FindFirstChild("Observe") then
            return true
        else
            return false
        end
    end
end

for _,v in pairs(Players:GetPlayers()) do
    if Check(v) == true then
        Notification.new("warning", "Illusionist Detected", ('Illusionist ' .. v.Name .. ' is in the the game.'))
        illusionistCounter = illusionistCounter +1
        table.insert(illusionists, illusionistCounter, v)
    end
    v.CharacterAdded:Connect(function()
        if not table.find(illusionists, v) then
            RunService.RenderStepped:Wait()
            if Check(v) == true then
                Notification.new("warning", "Illusionist Detected", ('Illusionist ' .. v.Name .. ' is in the the game.'))
                illusionistCounter = illusionistCounter +1
                table.insert(illusionists, illusionistCounter, v)
            end
        end
    end)
end

Players.PlayerAdded:Connect(function(plr)
	plr.CharacterAdded:Connect(function(char)
		if Check(plr) == true then
			Notification.new("warning", "Illusionist Joined", ('Illusionist ' .. plr.Name .. ' has joined the game.'))
			illusionistCounter = illusionistCounter +1
			table.insert(illusionists, illusionistCounter, plr)
		else
		end
	end)
end)

Players.PlayerRemoving:Connect(function(plr)
    if table.find(illusionists, plr) then
        for i,v in pairs(illusionists) do
            if v == plr then
                table.remove(illusionists, i)
                illusionistCounter = illusionistCounter -1
            end
        end
        Notification.new("warning", "Illusionist Left", ('Illusionist ' .. plr.Name .. ' has left the game.'))
    end
end)




Notification.new('success', 'Tidal Hub Premium', 'Reached end of file.', true, 10)



