-- Main Exploits

local MainExploits = TidalHub:Tab('Main Exploits')

local mouse = game.Players.LocalPlayer:GetMouse()
local userinput = game:GetService("UserInputService")
local speed = 250
local keybindToFly = Enum.KeyCode.Z
RunService = game:GetService("RunService")
getgenv().FlyEnabled = false
userinput.InputBegan:Connect(function(input,typing)
	if typing then
		return
	end
	if input.KeyCode == keybindToFly and getgenv().FlyEnabled then
		_G.Active = true
		RunService:BindToRenderStep("flyActive", 1, function()
			if _G.Active == false then RunService:UnbindFromRenderStep("flyActive") end
			game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = mouse.Hit.lookVector * speed
		end)
	end
end)

userinput.InputEnded:Connect(function(input)
	if input.KeyCode == keybindToFly then _G.Active = false end
end)

MainExploits:Toggle('[Z] Fly', function(t)
	getgenv().FlyEnabled = t
end)
MainExploits:Toggle("No Fall Damage",function(t)
	while t do task.wait()
        pcall(function()
			if t == true then
				game.Players.LocalPlayer.Character:FindFirstChild('CharacterHandler'):FindFirstChild('Remotes').FallDamage.Name = 'Ban'
			else
				game.Players.LocalPlayer.Character:FindFirstChild('CharacterHandler'):FindFirstChild('Remotes').Ban.Name = 'FallDamage'
			end
        end)
    end
end)


MainExploits:Toggle("No Kill Bricks",function(t)
    for _,v in pairs(workspace.Map:GetChildren()) do
        if v.Name == "KillBrick" or v.Name == "Lava" or v.Name == "PitKillBrick" or v.Name == "ArdorianKillbrick" or v.Name == "Fire" or v.Name == "KillFire" then
            if v:FindFirstChild("TouchInterest") then
                v.CanTouch = not t
            end
        end
    end
end)

MainExploits:Toggle('No Orderly Barriers', function(t)
	for _,v in pairs(workspace.Map:GetChildren()) do
        if v.Name == "OrderField" then
            if v:FindFirstChild("TouchInterest") then
                v.CanTouch = not t
				v.CanCollide = not t
            end
        end
    end
end)

MainExploits:Toggle('Trinket Auto-Pickup', function(t)
	getgenv().AutoPickup = t
	local PickupModule = {
		PickUpCooldown = 1,
		CanPick = true,
		TrinketFolder = workspace.Trinkets
	}
	while getgenv().AutoPickup do
		task.wait(.1)
		for _,v in pairs(workspace.Trinkets:GetChildren()) do
			if v:FindFirstChild("ClickPart") then
				local DistanceCheck = v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position
				if PickupModule.CanPick == true and DistanceCheck.Magnitude < 9 then 
					local Detector = v.ClickPart:FindFirstChild("ClickDetector")
					PickupModule.CanPick = false
					pcall(function()
					fireclickdetector(Detector)
					end)
					delay(.1,function()
						PickupModule.CanPick = true
					end)
				end
			end
		end
	end
end)


